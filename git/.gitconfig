[user]
	name = Nick Loadholtes
	email = nick@ironboundsoftware.com
[color]
	ui = auto
[color "status"]
	untracked = yellow bold
[core]
	editor = vi
	excludesfile = /home/njl/.gitignore
[alias]
	ci = commit
	al = log --pretty=oneline --graph --abbrev-commit --decorate
	st = status
	out = log origin/master..HEAD
	co = checkout
	branches = branch -a
	in = log HEAD..origin/master
	forgetabout = update-index --assume-unchanged
	remember = update-index --no-assume-unchanged
	undo = reset --soft HEAD~1
[push]
	default = current
